    "@astrojs/sitemap": "3.5.1",
    "@tailwindcss/vite": "4.1.12",
    "astro": "5.13.3",
    "astro-expressive-code": "0.41.3",
    "preact": "10.27.1",
    "rehype-autolink-headings": "7.1.0",
    "rehype-external-links": "3.0.0",
    "sharp": "0.34.3",
    "tailwindcss": "4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/typography": "0.5.16",
    "@types/react": "19.1.11",
