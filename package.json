    "astro-expressive-code": "0.35.3",
    "preact": "10.22.0",
    "rehype-autolink-headings": "7.1.0",
    "rehype-external-links": "3.0.0",
    "sharp": "0.33.4",
    "tailwindcss": "3.4.4"
  },
  "devDependencies": {
    "@tailwindcss/typography": "0.5.13",
    "@types/react": "18.3.3",
